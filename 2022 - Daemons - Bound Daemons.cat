      <selectionEntries>
        <selectionEntry id="f7f7-9947-281e-95d9" name="Warlord Trait: Skull Keeper" publicationId="cb13-da24-e6da-75b3" page="19" hidden="false" collective="false" import="true" type="upgrade">
          <modifiers>
            <modifier type="set" field="9afd-6d96-5451-9cd4" value="1">
              <conditions>
                <condition field="selections" scope="aa91-7f0d-cade-cbc5" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="0176-56a3-d590-b103" type="greaterThan"/>
