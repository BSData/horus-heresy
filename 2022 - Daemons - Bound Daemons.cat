            <modifier type="set" field="hidden" value="true">
              <conditions>
                <condition field="selections" scope="aa91-7f0d-cade-cbc5" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="0176-56a3-d590-b103" type="equalTo"/>
