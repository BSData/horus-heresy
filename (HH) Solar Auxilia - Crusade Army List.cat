    <profile id="f7733b9f-53b3-fea6-a353-06c729deb699" name="Auxilia Lasrifle" book="HH4: Conquest" page="247" hidden="false" profileTypeId="576561706f6e23232344415441232323">
      <profiles/>
      <rules/>
      <infoLinks/>
