        <entryLink id="96e8-84db-44d7-460a" name="Strategic Traits (HH- Rulebook p137)" hidden="false" collective="false" import="true" targetId="fb37-64c2-4afd-57da" type="selectionEntryGroup"/>
        <entryLink id="fd7b-9bed-41e4-53c7" name="Personal Traits (HH- Rulebook p137)" hidden="false" collective="false" import="true" targetId="b95a-7d48-eb20-9ba5" type="selectionEntryGroup"/>
      </entryLinks>
