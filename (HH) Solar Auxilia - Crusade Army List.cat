    <profile id="3c05e6b4-5c43-4815-d0f3-28ee932d80f0" name="Cohort Vexilla" publicationId="ca571888--pubN84158" page="250" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
