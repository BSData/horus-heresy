          <profiles>
            <profile id="12aa-65f5-c9b4-db9a" name="Apex Digital Lasers" publicationId="ca571888--pubN84158" page="87" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
              <characteristics>
