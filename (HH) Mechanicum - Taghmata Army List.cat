    <profile id="f4ffca2c-8bb4-2d0a-9f48-d0babbde81e7" name="Apocalypse Launcher" publicationId="ca571888--pubN66500" page="274" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Weapon">
      <characteristics>
        <characteristic name="Range" typeId="52616e676523232344415441232323">24&quot; - 360&quot;</characteristic>
