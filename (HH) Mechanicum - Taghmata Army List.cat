        <selectionEntry id="d129-5ede-d4fc-cd6d" name="Warp Shunt Field" publicationId="ca571888--pubN84158" page="223" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="maxSelections" type="max"/>
