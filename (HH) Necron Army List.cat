      <profiles>
        <profile id="4b03-c18b-5b20-ccba" name="Ziggurat" hidden="false" typeId="c2e2-42f3-9e28-c3fa" typeName="Fortification/Terrain">
          <characteristics>
            <characteristic name="Composition" typeId="d34f-9153-d292-adab">One Ziggurat</characteristic>
            <characteristic name="Terrain type" typeId="98d6-2143-3017-6d46">Impassable Large Building, AV14 on all sides</characteristic>
            <characteristic name="Access Points &amp; Fire Points" typeId="beac-711d-f76d-e254">None</characteristic>
