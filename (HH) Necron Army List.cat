    <selectionEntry id="d834-7113-0466-149b" name="Trazyn The Infinate" publicationId="6fc6-dd7c-7a39-b0c8" hidden="false" collective="false" import="true" type="upgrade">
      <profiles>
        <profile id="3c3d-8790-bedc-afaf" name="Trazyn The Infinate" publicationId="6fc6-dd7c-7a39-b0c8" hidden="false" typeId="556e697423232344415441232323" typeName="Unit">
          <characteristics>
            <characteristic name="Unit Type" typeId="556e6974205479706523232344415441232323">Infantry (Character)</characteristic>
            <characteristic name="WS" typeId="575323232344415441232323">5</characteristic>
